class Solution {
    public int[] twoSum(int[] nums, int target) {
        int n=nums.length;
        int a[]=new int[2];
        HashMap<Integer,Integer> map=new HashMap<>();
        for(int i=0;i<n;i++){
        if(map.containsKey(target-nums[i])){
            a[0]=i;
            a[1]=map.get(target-nums[i]);
        } else map.put(nums[i],i);}
        return a;
    }
}
